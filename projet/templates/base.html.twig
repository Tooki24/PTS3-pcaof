<!DOCTYPE html>
<html lang="fr" xml:lang="fr" xmlns="http://www.w3.org/1999/xhtml">
    <head>
        {% block title %}{% endblock %}
        {% block metaTag %}
        {% endblock %}

        <!-- Meta -->
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name='robots' content='index, follow, max-image-preview:large, max-snippet:-1, max-video-preview:-1' />

        <script type="application/ld+json">
        {
            "@context": "https://schema.org",
            "@type": "ProfessionalService",
            "name": "PCAoF",
            "image": " {{ asset('img/logo/logoColor.png') }}",
            "@id": "",
            "url": "https://pcaof.com/",
            "telephone": "",
            "address": {
                "@type": "PostalAddress",
                "streetAddress": "",
                "addressLocality": "La Rochelle",
                "postalCode": "17000",
                "addressCountry": "FR"
            },
            "sameAs": [
                "https://www.facebook.com/Popular-Culture-Association-of-France-240165316556260/",
                "https://www.instagram.com/pcaofrance/"
            ]
        }
    </script>

        <!-- Bootstrap core CSS -->
        <link href="{{ asset('css/bootstrap.min.css') }}" rel="stylesheet">

        <!-- Style Theme -->
        <link href="{{ asset('css/style.css') }}" rel="stylesheet">

        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css">

        <!-- Fav Icon-->
        <link rel="icon" href="{{ asset('img/favicon/favicon-32x32.png') }}" sizes="32x32" />
        <link rel="icon" href="{{ asset('img/favicon/favicon-192x192.png') }}" sizes="192x192" />
        <link rel="apple-touch-icon" href="{{ asset('img/favicon/favicon-180x180.png') }}" />

        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body class="d-flex flex-column h-100">
        {% include 'base/nav.html.twig' %}
        {% block body %}
        
        {% endblock %}
        {% include 'base/footer.html.twig' %}

    <!-- SCRIPT JS -->
        <script src="{{ asset('js/scripts.js') }}"></script>
        <script src="{{ asset ('js/bootstrap.bundle.min.js') }}"></script>
        <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    
    </body>
</html>
